    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder};${workspaceFolder}/build;C:\\Users\\Yunpeng\\AppData\\Local\\Programs\\Python\\Python313\\Lib",
                "PYTHONHOME": "${workspaceFolder};${workspaceFolder}/build;C:\\Users\\Yunpeng\\AppData\\Local\\Programs\\Python\\Python313\\Lib",
            },
        },
        {
            "name": "extension python",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../opensource/cpython/PCBuild/amd64/python_d.exe",
            "args": [
                "${workspaceFolder}/src/python/main.py"
            ],
            "console": "integratedTerminal",
            "environment": [
                {
                    "name": "PYTHONPATH",
                    "value": "${workspaceFolder}/build;C:\\Users\\Yunpeng\\AppData\\Local\\Programs\\Python\\Python313\\Lib"
                },
                {
                    "name": "PYTHONHOME",
                    "value": "${workspaceFolder}/build;C:\\Users\\Yunpeng\\AppData\\Local\\Programs\\Python\\Python313\\Lib"
                }
            ],
            "stopAtEntry": false,
        }
    ]
}